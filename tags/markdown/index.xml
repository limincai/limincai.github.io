      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;\tan&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;tan⁡&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;正切&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;\cot&lt;/td&gt;
