          &lt;td style=&#34;text-align: center&#34;&gt;正割&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;\csc&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;csc⁡&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;余割&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
